<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.PlayerAndGameController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="playAndGameInformation" prefHeight="349.0" prefWidth="600" stylesheets="@../css/TableViewTestCss.css"  HBox.hgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="player_game_id" prefWidth="70.0" text="player_game_id" />
            <TableColumn fx:id="game_id" prefWidth="70.0" text="game_id" />
            <TableColumn fx:id="player_id" prefWidth="70.0" text="player_id" />
            <TableColumn fx:id="playing_date" prefWidth="70.0" text="playing_date" />
            <TableColumn fx:id="score" prefWidth="70.0" text="score" />

        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>

</AnchorPane>
